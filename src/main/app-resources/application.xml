  <workflow id="workflow" title="conf1 WestAfrica-HYPE Forecast" abstract="Make 1-10 days hydrometeorological forecasts for the Niger river basin with the HYPE model. More info at http://hypeweb.smhi.se/niger-hype/forecast and http://www.smhi.net/hype/wiki.">
    <workflowVersion>1.0</workflowVersion>
    <node id="node_forecast">
      <job id="template_hypeapps"/>
      <sources>
        <source id="model" scope="runtime" refid="string:list"></source>
      </sources>
    </node>
  </workflow>
