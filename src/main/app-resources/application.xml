<?xml version="1.0" encoding="us-ascii"?>
<application xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" id="application">
  <jobTemplates>
    <jobTemplate id="template_hypeapps">
      <streamingExecutable>/application/node_forecast/run.R</streamingExecutable>
      <defaultParameters>
        <!-- The model config object may or may not differ between niger-hype and west-africa-hype. If they differ we need to handle this. Now this can manually be selected on H-TEP via drag and drop. -->
        <parameter id="model" title="HYPE model configuration" abstract="HYPE model configuration" minOccurs="0" maxOccurs="1" scope="runtime" type="LiteralData">https://recast.terradue.com/t2api/search/hydro-smhi/models?uid=9345ED73B72F49E6FF31B07B57013BC519210E24</parameter> <!-- ToDo: Rename id to modelConfig or similar -->

        <parameter id="hydmodel" title="Hydrological model" abstract="Hydrological model" minOccurs="1" maxOccurs="2" scope="runtime" type="LiteralData">
          <options>
            <option>Niger-HYPE</option>
            <option>WestAfrica-HYPE</option>
            <default>WestAfrica-HYPE</default> 
          </options>
        </parameter>
        
        <parameter id="methc" title="Meterological hindcast" abstract="Meterological hindcast dataset" minOccurs="1" maxOccurs="2" scope="runtime" type="LiteralData">
          <options>
            <option>GFD 1.3 (SMHI)</option>
            <option>HydroGFD 2.0 (SMHI)</option>
            <default>HydroGFD 2.0 (SMHI)</default> 
          </options>
        </parameter>

        <parameter id="metfc" title="Meterological forecast" abstract="Meterological forecast dataset" minOccurs="1" maxOccurs="1" scope="runtime" type="LiteralData">
          <options>
            <option>ECOPER (SMHI)</option>
            <default>ECOPER (SMHI)</default> 
          </options>
        </parameter>

        <parameter id="idate" title="Forecast issue date (yyyy-mm-dd)" abstract="Data available since 2016-06-01" minOccurs="1" maxOccurs="1" scope="runtime" type="LiteralData">2019-10-03</parameter>

        <parameter id="hcperiodlen" title="Hindcast period length (days)" abstract="Default 10 year" minOccurs="1" maxOccurs="1" scope="runtime" type="LiteralData">3650</parameter>
        <!-- <parameter id="hcperiodlen" title="Hindcast period length (days)" abstract="Default 10 year" minOccurs="1" maxOccurs="1" scope="runtime" type="LiteralData">200</parameter> -->

        <parameter id="runtype" title="Run type (Operational/Reforecast)" abstract="Operational - scheduled run for automatic operational production. Reforecast - re-run forecast or with other options" minOccurs="1" maxOccurs="1" scope="runtime" type="LiteralData">
          <options>
            <option>Operational</option>
            <option>Reforecast</option>
            <default>Reforecast</default> 
          </options>
        </parameter>

        <parameter id="variables" title="Output variables" abstract="Select output variables" minOccurs="1" maxOccurs="8" scope="runtime" type="LiteralData">
          <options>
            <option>River Discharge (m3/s) COUT</option>           
            <option>Air Temperature (C) CTMP</option>
            <option>Precipitation (mm/day) CPRC</option>
            <option>Evapotranspiration (mm/day) EVAP</option>
            <option>Total Runoff (mm/day) CRUN</option>
            <option>Soil Moisture (-) fraction of plant avalable water in the root zone SRFF</option>
            <option>Lake Water Level (m) WCOM</option>
            <option>Total Surface Water Area (km2) lakes rivers and floodplains CSWA</option>
            <default>River Discharge (m3/s) COUT</default> 
          </options>
        </parameter>

